CREATE TABLE `storage` ( `id` VARCHAR(20) NOT NULL , `date` DATE NOT NULL , `time` VARCHAR(15) NOT NULL , `ip` VARCHAR(30) NOT NULL , `b_name` VARCHAR(15) NOT NULL , `b_version` VARCHAR(15) NOT NULL , `b_platform` VARCHAR(15) NOT NULL , `device` VARCHAR(120) NOT NULL , `country` VARCHAR(20) NOT NULL , `city` VARCHAR(20) NOT NULL ) ENGINE = InnoDB;

